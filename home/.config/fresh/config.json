{
  "theme": "dark",
  "check_for_updates": true,
  "editor": {
    "tab_size": 2,
    "auto_indent": false,
    "line_numbers": true,
    "relative_line_numbers": false,
    "scroll_offset": 3,
    "syntax_highlighting": true,
    "line_wrap": true,
    "highlight_timeout_ms": 5,
